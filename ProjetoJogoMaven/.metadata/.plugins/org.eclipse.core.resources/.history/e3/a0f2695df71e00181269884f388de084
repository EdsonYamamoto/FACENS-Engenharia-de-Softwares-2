package builder;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class CriadorDeJogoTest {

	@Test
	void testParaDescricaoVazia() {
        System.out.println("testParaDescricaoVazia");
        String descricao = "";
        CriadorDeJogo instance = new CriadorDeJogo();
        CriadorDeJogo expResult = null;
        CriadorDeJogo result = instance.para(descricao);
        assertEquals(expResult, result);
	}
	
	@Test
	void testBasicoMultiplicar()
	{
		System.out.println();
		assertEquals(0, 0);
		assertEquals(1, 12);
		assertEquals(1, 232);
		
	}

	
	@Test
	void testBasicoDividir()
	{
		assertEquals(0, 0);
		assertEquals(1, 12);
		assertEquals(1, 232);
		
	}
	
	
}
